{
    "name": "shirokovnv/laravel-query-api-backend",
    "description": "Unified API for running queries with eloquent models from any http client",
    "license": "MIT",
    "authors": [
        {
            "name": "Nickolai Shirokov",
            "email": "shirokovnv@gmail.com",
            "homepage": "https://github.com/shirokovnv"
        }
    ],
    "homepage": "https://github.com/shirokovnv/laravel-query-api-backend",
    "keywords": ["Laravel", "LaravelQueryApiBackend"],
    "require": {
        "illuminate/support": "~5.8|~6|~7|~8",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Shirokovnv\\LaravelQueryApiBackend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shirokovnv\\LaravelQueryApiBackend\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shirokovnv\\LaravelQueryApiBackend\\LaravelQueryApiBackendServiceProvider"
            ],
            "aliases": {
                "LaravelQueryApiBackend": "Shirokovnv\\LaravelQueryApiBackend\\Facades\\LaravelQueryApiBackend"
            }
        }
    }
}
